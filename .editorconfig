# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Tab indentation (no size specified)
[*.{js,css, html, package.json, .travis.yml}]
charset = utf-8
indent_style = space
indent_style = space
indent_size = 2
